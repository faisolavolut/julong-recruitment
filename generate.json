[
  // format data tablelist
  {
    "name": "mpr",
    "title": "MPR Overview",
    "url_root": "/d",
    "apix": {
      "port": "recruitment",
      "value": "data.data.mp_request_header",
      "path": "/api/mp-requests",
      "validate": "array"
    },
    "column": [
      ["id_applicant", "ID Applicant"],
      ["applicant_name", "Applicant Name"],
      ["gpa", "GPA"],
      ["major", "Major"],
      ["job_name", "Job Name"],
      ["job_experience", "Job Experience"],
      ["work_experience", "Work Experience (month)"],
      ["cv", "CV"],
      ["status_selection", "Status Selection", "status"]
    ]
  },
  // FORM NEW
  {
    "mode": "new",
    "name": "schedule-test",
    "title": "Schedule Test",
    "url_root": "/d/test-selection",
    "apix": {
      "port": "recruitment",
      "value": "data.data",
      "path": "/api/schedule-test",
      "validate": "object"
    },
    // default tipe fieldnya text
    // jika tipe fieldnya richtext maka col-span-2
    "field": [
      ["document_number", "Document Number", "text"], // disabled
      ["schedule_date", "Schedule Date", "date"],
      ["name", "Name", "text"],
      [
        "select_test_type",
        "Select Test Type",
        "dropdown",
        {
          "apix": {
            "port": "recruitment",
            "value": "data.data",
            "path": "/api/test-types",
            "keys": {
              "label": "name"
            },
            "validate": "dropdown"
          }
        }
      ],
      ["project_number", "Project Number", "text"], // disabled
      [
        "activity",
        "Activity",
        "dropdown",
        {
          "apix": {
            "port": "recruitment",
            "value": "data.data",
            "path": "/api/template-questions/form-types",
            "keys": {
              "label": "value",
              "value": "value"
            },
            "validate": "dropdown"
          }
        }
      ],

      ["start_date", "Start Date", "date"], // disabled
      ["end_date", "End Date", "date"], // disabled
      ["start_time", "Start Time", "time"],
      ["end_time", "End Time", "time"],
      ["location", "Location (Url)", "text"],
      ["job_name", "Job Name", "text"],
      ["description", "Description", "textarea"],
      ["link_test", "Link Test", "text"],
      ["status", "Status", "text"],
      ["total_candidate", "Total Candidate", "money"]
    ]
  },

  // FORM EDIT
  {
    "mode": "edit",
    "name": "applicant-overview",
    "title": "Applicant Overview",
    "url_root": "/d/administrative",
    "apix": {
      "port": "recruitment",
      "value": "data.data",
      "path": "/api/job-postings",
      "validate": "object"
    },
    "onload": {
      "apix": {
        "port": "recruitment",
        "value": "data.data",
        "path": "/api/job-postings",
        "validate": "object"
      }
    },
    // default tipe fieldnya text
    // jika tipe fieldnya richtext maka col-span-2

    "field": [
      ["project_number", "Project Number", "text"], // disabled
      [
        "activity",
        "Activity",
        "dropdown",
        {
          "apix": {
            "port": "recruitment",
            "value": "data.data",
            "path": "/api/template-questions/form-types",
            "keys": {
              "label": "value",
              "value": "value"
            },
            "validate": "dropdown"
          }
        }
      ],
      ["start_date", "Start Date", "date"], // disabled
      [
        "pic",
        "PIC",
        "dropdown",
        {
          "apix": {
            "port": "portal",
            "value": "data.data.employees",
            "path": "/api/employees",
            "keys": {
              "label": "name"
            },
            "validate": "dropdown"
          }
        }
      ],
      ["status", "Status", "text"],
      ["document_date", "Document Date", "date"],
      ["document_number", "Document Number", "text"], // disabled
      ["end_date", "End Date", "date"], // disabled
      ["total_candidate", "Total Candidate", "money"]
    ]
  }
]
